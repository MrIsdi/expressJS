<!doctype html>
<html lang="en">
    <head>
        <title><%= data[0].name %> </title>
        <!-- Required meta tags -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <!-- Bootstrap CSS v5.0.2 -->
        <link rel="stylesheet" href="/stylesheets/style.css">
        <link href='https://unpkg.com/boxicons@2.1.1/css/boxicons.min.css' rel='stylesheet'>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"  integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    </head>
    <body>
        <% include sidebar.ejs %> 
        <div class="row justify-content-center content bgBase hvh">
            <div class="col-6">
                <p class="fs-2 text-center mt-3 text-white">Participant ORKMTZ: <br><%= data[0].name %></p>
                <div class="table-resposive tableNeu p-3">
                    <table class="table table-borderless text-white">
                        <thead>
                            
                        </thead>
                        <tbody>
                            <tr>
                                <td>Key</td>
                                <td>Value</td>
                            </tr>
                            <tr>
                                <td>Nama Lengkap</td>
                                <td><%= data[0].name %> </td>
                            </tr>
                            <tr>
                                <td>NIM</td>
                                <td><%= data[0].nim %> </td>
                            </tr>
                            <tr>
                                <td>Email</td>
                                <td><%= data[0].email %> </td>
                            </tr>
                            <tr>
                                <td>Jurusan</td>
                                <td><%= data[0].jurusan %> </td>
                            </tr>
                            <tr>
                                <td>Tanggal Lahir</td>
                                <td><%= data[0].tLahir %> </td>
                            </tr>
                            <tr>
                                <td>Tempat Lahir</td>
                                <td><%= data[0].tmpLahir %> </td>
                            </tr>
                            <tr>
                                <td>Asal Daerah</td>
                                <td><%= data[0].asal %> </td>
                            </tr>
                            <tr>
                                <td>Cita-cita</td>
                                <td><%= data[0].citaCita %> </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <a href="/admin" class="btn text-white btnNeu mt-3">Kembali</a>
            </div>
        </div>

        <!-- Bootstrap JavaScript Libraries -->
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js" integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js" integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
    </body>
</html>